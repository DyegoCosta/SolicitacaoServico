<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping.dtd">  
<hibernate-mapping>  
    <class name="Usuario" table="Usuarios">  
        <id name="usuarioId" column="usuarioId" type="int">  
            <generator class="assigned"/>  
        </id>  
        <property name="cargo" type="string"/>      
        <property name="login" type="string"/>  
        <property name="nome" type="string"/>  
        <property name="sobrenome" type="string"/>  
        <property name="senha" type="string"/>  
        <property name="status" type="string"/>
    </class>  
</hibernate-mapping>        